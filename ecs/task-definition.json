{
  "family": "vibelist-backend",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::257976284139:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "spring-backend",
      "image": "<IMAGE_URI>",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "environment": [
        { "name": "DB_SERVER", "value": "REPLACE_ME" },
        { "name": "DB_PORT", "value": "REPLACE_ME" },
        { "name": "DB_NAME", "value": "REPLACE_ME" },
        { "name": "DB_USERNAME", "value": "REPLACE_ME" },
        { "name": "DB_PASSWORD", "value": "REPLACE_ME" },
        { "name": "GOOGLE_CLIENT_ID", "value": "REPLACE_ME" },
        { "name": "GOOGLE_CLIENT_SECRET", "value": "REPLACE_ME" },
        { "name": "KAKAO_CLIENT_ID", "value": "REPLACE_ME" },
        { "name": "KAKAO_CLIENT_SECRET", "value": "REPLACE_ME" },
        { "name": "SPOTIFY_CLIENT_ID", "value": "REPLACE_ME" },
        { "name": "SPOTIFY_CLIENT_SECRET", "value": "REPLACE_ME" },
        { "name": "ES_HOST", "value": "REPLACE_ME" }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024"
}